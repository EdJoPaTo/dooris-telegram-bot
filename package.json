		"telegraf": "^4.1.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.9.0",
		"@types/node": "^14.0.5",
		"del-cli": "^3.0.0",
		"typegram": "^3.1.9",
		"typescript": "^4.1.2",
		"xo": "^0.38.2"
	},
	"xo": {
		"rules": {
			"ava/no-ignored-test-files": "off"
		}
	}
